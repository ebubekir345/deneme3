{
  "name": "maestro",
  "description": "Maestro",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node --max_old_space_size=16384 scripts/build.js",
    "clean:map": "rimraf ./build/**/*.map",
    "test": "node --max_old_space_size=16384 scripts/test.js --env=jsdom",
    "test:coverage": "node --max_old_space_size=16384 scripts/test.js --env=jsdom --coverage",
    "test:watch": "node scripts/test.js --env=jsdom --watchAll",
    "postinstall": "node scripts/postinstall.js",
    "swagger:generate": "openapi-generator-cli generate -i http://localhost:8076/swagger/v1/swagger.json -g typescript-axios -o src/services/swagger --skip-validate-spec",
    "prettier": "prettier --config .prettierrc.json --write {src,__{tests}__}/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint src/** --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src/** --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --fix",
    "i18n-parser": "i18next --config i18next-parser.config.js"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.22.2",
    "@elastic/apm-rum": "^5.9.1",
    "@emotion/core": "10.0.28",
    "@oplog/data-grid": "0.3.51",
    "@oplog/express": "^1.0.0-alpha.46",
    "@oplog/file-uploader": "0.1.6",
    "@oplog/resource-redux": "0.2.6",
    "axios": "0.21.1",
    "dynamic-query-builder-client": "0.2.4",
    "i18next": "20.2.2",
    "i18next-browser-languagedetector": "^7.0.1",
    "lottie-web": "5.7.0",
    "memoize-one": "^5.2.1",
    "moment": "2.29.1",
    "qrcode.react": "^3.1.0",
    "react": "17.0.2",
    "react-barcode-reader": "0.0.1",
    "react-barcodes": "1.0.3",
    "react-chrono": "1.10.1",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-keys": "^2.7.2",
    "react-i18next": "11.8.15",
    "react-intl": "2.8.0",
    "react-intl-redux": "2.2.0",
    "react-loading-skeleton": "2.2.0",
    "react-redux": "7.2.4",
    "react-router": "4.2.0",
    "react-router-dom": "5.1.2",
    "react-simple-keyboard": "2.2.40",
    "react-use-websocket": "^4.5.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "redux-saga": "0.16.0",
    "use-global-hook": "0.2.1",
    "yup": "0.28.1"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "cheerio": "1.0.0-rc.10",
    "es-cookie": "1.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@openapitools/openapi-generator-cli": "2.1.26",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/react-hooks": "3.7.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-helmet": "^6.1.1",
    "@types/react-intl": "2.3.15",
    "@types/react-intl-redux": "0.1.11",
    "@types/react-redux": "7.1.16",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.1.2",
    "@types/react-router-redux": "5.0.17",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/redux-saga": "0.10.5",
    "@types/use-global-hook": "0.1.2",
    "@types/yup": "0.26.36",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "0.3.1",
    "babel-preset-react-app": "10.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "cheerio": "1.0.0-rc.10",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "es-cookie": "1.3.2",
    "eslint": "7.21.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "2.5.2",
    "fork-ts-checker-webpack-plugin": "6.1.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "5.2.0",
    "i18next-parser": "3.11.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "^2.5.0",
    "jest-pnp-resolver": "1.2.2",
    "jest-resolve": "26.6.2",
    "jest-watch-typeahead": "0.6.1",
    "mini-css-extract-plugin": "1.3.9",
    "prettier": "1.19.1",
    "react-app-polyfill": "0.2.1",
    "react-dev-utils": "11.0.4",
    "react-router-test-context": "0.1.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "16.9.0",
    "redux-devtools-extension": "2.13.7",
    "redux-mock-store": "1.5.3",
    "replace-in-file": "6.2.0",
    "resolve": "1.10.0",
    "terser-webpack-plugin": "5.1.1",
    "ts-jest": "26.5.2",
    "ts-loader": "^9.3.0",
    "typescript": "4.7.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "4.0.0-beta.2",
    "webpack-manifest-plugin": "3.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {
        "no-unused-vars": true
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "jest-canvas-mock"
    ],
    "testMatch": [
      "<rootDir>/src/**/tests/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/config/"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "./node_modules/jest-watch-typeahead/filename.js",
      "./node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}